#
# implizite Regeln
#
.cpp.o:
	@./cfile 0 $<
	@$(CXX_PARAM) $<

.cpp.d:
	@echo "-" $(HOST)": Generating dependencies for" $@ "-"
	@$(SHELL) -ec 'gcc $(DEP_INFOS) $(DEFINE) $(INCLUDE) \
		$< | sed '\''s/\($*\)\.o[ :]*/\1.o $@ : /g'\'' > $@; \
		[ -s $@ ] || $(RM) $@'

%.moc.cpp: %.h
	@$(MOC) $< -o $@

