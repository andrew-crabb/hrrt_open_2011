	@./cfile 4 ecat7_global.h
	@./cfile 4 fastmath.h
	@./cfile 4 global_tmpl.cpp
	@./cfile 4 global_tmpl.h
	@./cfile 4 matrix.cpp
	@./cfile 4 matrix.h
	@./cfile 4 rebin_x.cpp
	@./cfile 4 rebin_x.h
	@./cfile 4 reconbe_wrapper.cpp
	@./cfile 4 reconbe_wrapper.h
	@./cfile 4 red_const.h
	@./cfile 4 registry.h
	@./cfile 4 setup.cpp
	@./cfile 4 timestamp.h
	@./cfile 4 types.h
	@./cfile 4 vecmath.h
	@./cfile 4 vecmath.cpp

