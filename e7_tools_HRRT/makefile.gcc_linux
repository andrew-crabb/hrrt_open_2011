cfile.o: cfile.cpp
	@echo "-" $(HOST)": compiling cfile.cpp -"
	@$(CXX_PARAM) cfile.cpp

cfile: cfile.o
	@echo "-" $(HOST)": linking cfile     -"
	@$(LINK) cfile.o -o $@
	@-strip $@
#	@gzexe $@
	@echo "=" $(HOST)": cfile linked      ="
	@echo
	@./cfile 4 makefile
	@./cfile 4 commands.h
	@./cfile 4 const.h
	@./cfile 4 dynamic_link.cpp
	@./cfile 4 dynamic_link.h
	@./cfile 4 ecat_tmpl.cpp
